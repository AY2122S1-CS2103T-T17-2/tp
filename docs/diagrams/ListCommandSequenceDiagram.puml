@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant "__:LogicManager__" as LogicManager LOGIC_COLOR
participant "__:AddressBookParser__" as AddressBookParser LOGIC_COLOR
participant "__:ListCommandParser__" as ListCommandParser LOGIC_COLOR
participant "__l:ListCommand__" as ListCommand LOGIC_COLOR
participant "__:CommandResult__" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "__:Model__" as Model MODEL_COLOR
end box

[-> LogicManager : execute\n("list s")
activate LogicManager
LogicManager -> AddressBookParser : parseCommand\n("list s")
activate AddressBookParser

create ListCommandParser
AddressBookParser -> ListCommandParser
activate ListCommandParser

AddressBookParser <-- ListCommandParser
deactivate ListCommandParser

AddressBookParser -> ListCommandParser : parse(" s")
activate ListCommandParser

create ListCommand
ListCommandParser -> ListCommand
activate ListCommand

ListCommandParser <-- ListCommand : l
deactivate ListCommand

ListCommandParser --> AddressBookParser : l
deactivate ListCommandParser

AddressBookParser --> LogicManager : l
deactivate AddressBookParser
destroy ListCommandParser

LogicManager -> ListCommand : execute(model)
activate ListCommand

ListCommand -> Model : List all students
activate Model
ListCommand <-- Model
deactivate Model

create CommandResult
ListCommand -> CommandResult
activate CommandResult

CommandResult --> ListCommand
deactivate CommandResult

ListCommand --> LogicManager : result
deactivate ListCommand

[<--LogicManager
deactivate LogicManager
ListCommand -[hidden]-> LogicManager
destroy ListCommand
@enduml
